<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="825.0" prefWidth="1316.0" style="-fx-background-image: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Mohseni.Mohseni_Profiles">
    <children>
        <Button fx:id="HomeButton" layoutX="1153.0" mnemonicParsing="false" onAction="#HomeButtonFunction" prefHeight="33.0" prefWidth="46.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <ColorPicker fx:id="colorPicker" layoutX="1199.0" onAction="#ChangingColor" prefHeight="42.0" prefWidth="117.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></ColorPicker>
        <Label fx:id="timeShowLabel" layoutX="911.0" prefHeight="42.0" prefWidth="242.0">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <Button fx:id="LogOutButton" layoutX="1259.0" layoutY="777.0" mnemonicParsing="false" onAction="#LogOutFunction" prefHeight="47.0" prefWidth="57.0" style="-fx-background-color: #FC0216;">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <ImageView fx:id="connectionStatus" fitHeight="42.0" fitWidth="149.0" layoutX="7.0" layoutY="783.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="reconnectionButton" layoutX="1.0" layoutY="770.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="68.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <ListView fx:id="listView" layoutX="316.0" layoutY="112.0" prefHeight="642.0" prefWidth="630.0" />
      <MenuBar fx:id="MenuBar" prefHeight="42.0" prefWidth="915.0">
         <menus>
            <Menu mnemonicParsing="false" text="Students profile">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#ProfilesFunction" text="profile" />
               </items>
            </Menu>
         </menus>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </MenuBar>
      <TextField fx:id="textFieldForSearch" layoutX="565.0" layoutY="61.0" />
    </children>
</AnchorPane>
